Host *
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa

Host github
    HostName github.com
    IdentityFile ~/.ssh/id_ed25519
    User git

Host gitlab
    HostName git.ileodo.com
    User git

Host ileodo
    HostName ileodo.com
    IdentitiesOnly yes
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist 8h
    User git

Include ~/._env/.ssh/config
Include ~/._local/.ssh/config
